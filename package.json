{
  "name": "myapp",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start:dev": "SET \"NODE_ENV=development\" SET DEBUG=myapp: * & nodemon ./bin/www",
    "db:create:migration": "npx sequelize migration:generate --name ",
    "db:migrate": "npx sequelize db:migrate",
    "db:rollback": "npx sequelize db:migrate:undo",
    "db:undo:all" : "npx sequelize db:migrate:undo:all", 
    "postinstall": "npx sequelize db:migrate"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "pg-promise": "^10.9.4",
    "pug": "2.0.0-beta11",
    "sequelize": "^6.6.2",
    "sequelize-auto": "^0.8.1",
    "sequelize-cli": "^6.2.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.7"
  }
}
